{
  "name": "server-clean-code",
  "version": "1.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc",
    "debug": "nodemon -L --watch dist --inspect=0.0.0.0:9229 --nolazy dist/main/server.js",
    "local": "sucrase-node src/main/server.ts",
    "docker:up": "npm run build && docker-compose up -d",
    "docker:up:watch": "npm run build && docker-compose up",
    "docker:down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --no-cache --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "keywords": [],
  "author": "Eduardo Bergantini Pinto",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.5",
    "@types/bcrypt": "^3.0.1",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.3",
    "@types/mongodb": "^3.6.19",
    "@types/node": "^14.17.4",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.6.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "eslint": "^7.30.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^3.2.6",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "rimraf": "^3.0.2",
    "sucrase": "^3.19.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.9",
    "nodemon": "^2.0.9",
    "validator": "^13.6.0"
  },
  "engines": {
    "node": "14.x"
  }
}
